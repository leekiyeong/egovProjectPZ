#modalLoginWrapper h2 {
    margin-bottom: 15px;
}

.input-container {
    position: relative;
    margin-bottom: 15px;
}

.input-field {
    padding: 8px;
    width: calc(100% - 16px); /* 좌우 8px 여백 제외 */
    border: 1px solid #ccc;
    border-radius: 5px;
    font-size: 16px;
    transition: border-color 0.2s;
}

.input-field:focus {
    border-color: #3498db;
}

.input-field:focus + .input-label,
.input-field:not(:placeholder-shown) + .input-label {
    top: -10px;
    left: 5px;
    font-size: 12px;
    color: #3498db;
    background-color: white;
    padding: 0 5px;
}

