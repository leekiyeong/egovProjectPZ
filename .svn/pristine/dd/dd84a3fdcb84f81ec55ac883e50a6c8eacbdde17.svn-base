    display: flex;
    align-items: center;
}

.email-input input,
.email-input select {
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 5px;
    margin-right: 5px;
}

.email-input span {
    font-weight: bold;
}

.input-with-button {
    display: flex;
    align-items: center;
}

.input-with-button input,
.input-with-button select {
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 5px;
    margin-right: 5px;
}

.input-with-button button {
    padding: 8px 10px;
    background-color: #3498db;
    color: white;
    border: none;
    border-radius: 5px;
    cursor: pointer;
}

/* 회원가입 전송 박스 */
#signUpSubmit {
    padding: 10px 20px;
    background-color: #3498db;
    color: white;
