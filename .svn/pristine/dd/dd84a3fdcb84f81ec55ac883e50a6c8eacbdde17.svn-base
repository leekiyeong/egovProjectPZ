/* 회원가입창 */
#modalSignUp {
	display: none;
	position: fixed;
	left: 50%;
	top: 35%;
	transform: translate(-50%, -50%);
	z-index: 101;
	background-color: white;
	padding: 20px;
	border-radius: 30px;
	width: 500px;
	height: 500px;
}

/* 회원가입 모달 내부 스타일 */
#modalSignUpWrapper {
    display: flex;
    flex-direction: column;
    gap: 10px;
    padding: 20px;
    background-color: white;
    border-radius: 20px;
    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);
    text-align: left;
}

#modalSignUpWrapper label {
    font-weight: bold;
}

#modalSignUpWrapper input {
    padding: 8px;
    width: 65%;
    border: 1px solid #ccc;
    border-radius: 5px;
}

/* 연락처 입력란 스타일 */
.phone-input {
    display: flex;
    align-items: center;
}

.phone-input input {
    width: 70px; /* 각 입력필드 너비 조정 */
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 5px;
}

.phone-input span {
    margin: 0 5px;
    font-weight: bold;
}

/* 이메일관련 css */
